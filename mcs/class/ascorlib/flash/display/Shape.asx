package flash.display {

	public class Shape extends DisplayObject {
	
		//
		// Properties
		//
	
		public property graphics : Graphics 
		{ 
			get {
				if (!mGraphics) {
					mGraphics = new Graphics();
				} 
				return mGraphics; 
			} 
		}
		
		
		private var mGraphics:Graphics = null;
	
	}

}
